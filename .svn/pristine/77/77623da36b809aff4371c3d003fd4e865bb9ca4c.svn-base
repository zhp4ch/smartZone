package com.mass.biz.smart.services.Increment.model;

import java.io.Serializable;
import java.util.Date;

/**
 * @ClassName 增值服务
 * @Author 郑上进
 * @Date 2018/8/13 14:18
 * @Version 1.0
 **/
public class Increment implements Serializable {

    /*系统*/
    /**
     * 主键id
     */
    private Long id;

    /**
     * 创建时间
     */
    private Date createTime;

    /**
     * 删除状态
     */
    private Integer delFlag;

    /*增值服务*/
    /**
     * 标题
     */
    private String title;

    /**
     * 内容描述
     */
    private String content;

    /**
     * 申请人
     */
    private Integer applyUserId;

    /**
     * 申请人姓名
     */
    private String applyName;

    /**
     * 联系电话
     */
    private String phone;

    /**
     * 处理状态(0.未处理，1.已处理)
     */
    private Integer incrementState;

    public String getApplyName() {
        return applyName;
    }

    public void setApplyName(String applyName) {
        this.applyName = applyName;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Integer getDelFlag() {
        return delFlag;
    }

    public void setDelFlag(Integer delFlag) {
        this.delFlag = delFlag;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Integer getApplyUserId() {
        return applyUserId;
    }

    public void setApplyUserId(Integer applyUserId) {
        this.applyUserId = applyUserId;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public Integer getIncrementState() {
        return incrementState;
    }

    public void setIncrementState(Integer incrementState) {
        this.incrementState = incrementState;
    }
}
